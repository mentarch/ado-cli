{
  "name": "ado-cli",
  "version": "0.1.0",
  "description": "Azure DevOps CLI tool with GitHub CLI compatibility",
  "main": "dist/index.js",
  "bin": {
    "ado": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build"
